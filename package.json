{
  "name": "dispose-me",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "author": "Kamil Dybicz",
  "license": "MIT",
  "main": "src/handler.js",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "prestart": "yarn",
    "start": "sls offline start --dontPrintOutput --noPrependStageInUrl --noAuth",
    "predebug": "yarn",
    "debug": "env SLS_DEBUG=* sls offline --noPrependStageInUrl --noAuth",
    "lint": "eslint src -c .eslintrc.json --ext ts",
    "test": "jest --watch",
    "test:coverage": "jest --all --coverage",
    "predeploy": "yarn && yarn lint && jest --all",
    "deploy": "sls deploy --stage"
  },
  "dependencies": {
    "address-rfc2822": "^2.0.5",
    "aws-sdk": "^2.785.0",
    "email-addresses": "^3.1.0",
    "mailparser": "^3.0.0",
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "@types/serverless": "^1.72.5",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "serverless-domain-manager": "^5.1.0",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "6.6.0",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  }
}
